#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/matteo.granzotto/Code/Work/Wavelop/next.js/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="$NODE_PATH:/Users/matteo.granzotto/Code/Work/Wavelop/next.js/node_modules/.pnpm/node_modules"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../node_modules/.pnpm/nanoid@3.1.32/node_modules/nanoid/bin/nanoid.cjs" "$@"
else
  exec node  "$basedir/../../../../node_modules/.pnpm/nanoid@3.1.32/node_modules/nanoid/bin/nanoid.cjs" "$@"
fi
